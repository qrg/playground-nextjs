{
  "name": "playground-nextjs-ts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "echo \"no test specified\" && exit 0",
    "lint": "yarn type-check && yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "next lint",
    "lint:prettier": "prettier --check '**/*.{js,jsx,ts,tsx,md,yml,yaml,html,json}'",
    "type-check": "tsc --noEmit",
    "format": "yarn format:eslint && yarn format:prettier",
    "format:eslint": "next lint --fix",
    "format:prettier": "prettier --write '**/*.{js,jsx,ts,tsx,md,yml,yaml,html,json}'"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/material": "^5.9.3",
    "next": "12.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.0.6",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "12.2.4",
    "eslint-config-prettier": "^8.5.0",
    "msw": "^0.44.2",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
